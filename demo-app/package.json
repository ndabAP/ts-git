    "browserfs": "1.4.3",
    "core-js": "2.6.11",
    "i": "0.3.6",
    "lodash": "4.17.15",
    "npm": "6.13.2",
    "vue": "2.6.10",
    "vue-class-component": "7.1.0",
    "vue-command": "7.0.7",
    "vue-monaco": "1.1.0",
