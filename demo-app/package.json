{
  "name": "ts-git-demo-app",
  "version": "0.1.0",
  "private": true,
  "description": "A demo app for ts-git, written in Vue and TypeScript",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "node --max_old_space_size=2048 node_modules/.bin/vue-cli-service build",
    "tslint": "tslint -c ../tslint.json -p .",
    "tslint-fix": "tslint -c ../tslint.json -p . --fix",
    "prettier": "prettier --check '**/*.ts'",
    "prettier-fix": "prettier --write '**/*.ts'",
    "lint": "npm run prettier; npm run tslint",
    "lint-fix": "npm run prettier-fix; npm run tslint-fix"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/nfriend/ts-git.git"
  },
  "keywords": [
    "git",
    "typescript",
    "javascript",
    "vue",
    "demo",
    "example"
  ],
  "author": "Nathan Friend",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/nfriend/ts-git/issues"
  },
  "homepage": "https://gitlab.com/nfriend/ts-git#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
